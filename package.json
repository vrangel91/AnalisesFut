{
  "name": "boasvindasbotbet",
  "version": "1.0.0",
  "description": "IA para apostas de futebol usando API-SPORTS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "db:init": "node src/database/init.js",
    "db:migrate": "node src/database/migrate.js",
    "test:cache": "node test-cache.js",
    "test:predictions": "node test-predictions-cache.js",
    "test:api": "node test-cached-api.js",
    "test:database": "node test-database-cache.js",
    "test:all": "node run-all-tests.js",
    "ngrok": "node scripts/ngrok.js",
    "ngrok:start": "node -e \"require('./src/services/ngrokService').startTunnel()\"",
    "ngrok:stop": "node -e \"require('./src/services/ngrokService').stopTunnel()\"",
    "ngrok:status": "node -e \"console.log(JSON.stringify(require('./src/services/ngrokService').getTunnelInfo(), null, 2))\""
  },
  "dependencies": {
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.2",
    "rate-limiter-flexible": "^2.4.2",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "futebol",
    "apostas",
    "ia",
    "api-sports",
    "predicoes"
  ],
  "author": "Seu Nome",
  "license": "MIT"
}
